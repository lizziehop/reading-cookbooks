toctoc_att.media.attribute.width =
  ## Where the media are displayed, indicates the display width 
  attribute width { toctoc_data.outputMeasurement }?
toctoc_att.media.attribute.height =
  ## Where the media are displayed, indicates the display height
  attribute height { toctoc_data.outputMeasurement }?
toctoc_att.media.attribute.scale =
