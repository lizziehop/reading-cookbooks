toctoc_moduleRef =
  ## (module reference) references a module which is to be incorporated into a schema.
  element ns1:moduleRef {
    (toctoc_content?)
    >> sch:pattern [
         id = "moduleRef-constraint-not-key-and-url"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:moduleRef"
           "\x{a}" ~
           "                  "
           sch:report [
             test = "@url and @key"
             "\x{a}" ~
             "		    only the key= OR the url= attribute of moduleRef should be supplied, not both\x{a}" ~
             "		  "
           ]
           "\x{a}" ~
           "                "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id = "moduleRef-constraint-not-include-and-except"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:moduleRef"
           "\x{a}" ~
           "                  "
           sch:report [
             test = "@except and @include"
             "\x{a}" ~
             "		    only the exclude= OR the include= attribute of moduleRef should be supplied, not both\x{a}" ~
             "		  "
           ]
           "\x{a}" ~
           "                "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id = "moduleRef-constraint-if-url-then-prefix"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:moduleRef[@url]"
           "\x{a}" ~
           "                  "
           sch:assert [
             test = "@prefix"
             "\x{a}" ~
             "		    a moduleRef that uses the url= attribute should have a prefix= attribute, too\x{a}" ~
             "		  "
           ]
           "\x{a}" ~
           "                "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id = "moduleRef-constraint-no-duplicate-modules"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:moduleRef[@key]"
           "\x{a}" ~
           "                  "
           sch:let [ name = "mykey" value = "@key" ]
           "\x{a}" ~
           "                  "
           sch:report [
             test = "preceding-sibling::tei:moduleRef[@key eq $mykey]"
             "The '"
             sch:value-of [ select = "@key" ]
             "' module is included (by reference) more than once"
           ]
           "\x{a}" ~
           "                "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id = "moduleRef-constraint-element-is-in-module"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:moduleRef[@key eq 'header']"
           "\x{a}" ~
           "                  "
           sch:let [
             name = "include"
             value = "tokenize( normalize-space(@include),' ')"
           ]
           "\x{a}" ~
