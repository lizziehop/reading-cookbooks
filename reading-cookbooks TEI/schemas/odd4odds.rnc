           "sequenceOptional" | ## members of the class may be provided one or more times, in sequence, but are optional. 
           "sequenceOptionalRepeatable" | ## members of the class may be provided one or more times, in sequence
           "sequenceRepeatable")?,
        (
         ## members of the class are alternatives
         "alternation" | ## members of the class are to be provided in sequence
           "sequence" | ## members of the class may be provided, in sequence, but are optional
           "sequenceOptional" | ## members of the class may be provided one or more times, in sequence, but are optional. 
           "sequenceOptionalRepeatable" | ## members of the class may be provided one or more times, in sequence
           "sequenceRepeatable")?,
        (
         ## members of the class are alternatives
         "alternation" | ## members of the class are to be provided in sequence
           "sequence" | ## members of the class may be provided, in sequence, but are optional
           "sequenceOptional" | ## members of the class may be provided one or more times, in sequence, but are optional. 
           "sequenceOptionalRepeatable" | ## members of the class may be provided one or more times, in sequence
           "sequenceRepeatable")?,
        (
         ## members of the class are alternatives
         "alternation" | ## members of the class are to be provided in sequence
           "sequence" | ## members of the class may be provided, in sequence, but are optional
           "sequenceOptional" | ## members of the class may be provided one or more times, in sequence, but are optional. 
           "sequenceOptionalRepeatable" | ## members of the class may be provided one or more times, in sequence
           "sequenceRepeatable")?
      }
    }?,
    empty
  }

toctoc_macroSpec =
  ## (macro specification) documents the function and implementation of a pattern. 
  element ns1:macroSpec {
    ((toctoc_model.glossLike | toctoc_model.descLike)*,
     (toctoc_content | toctoc_valList)*,
     toctoc_constraintSpec*,
     toctoc_exemplum*,
     toctoc_remarks*,
     toctoc_listRef*)
    >> sch:pattern [
         id = "macroSpec-constraint-macrospec-in-module"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:macroSpec"
           "\x{a}" ~
           "               "
           sch:assert [
             test =
               "not( @module )               or               //tei:moduleSpec[ @ident = current()/@module ]               or               //tei:moduleRef[ @key = current()/@module ]"
             "Macro "
             sch:value-of [ select = "@ident" ]
             ': the value of the module attribute  ("'
             sch:value-of [ select = "@module" ]
             '")\x{a}' ~
             "should correspond to an existing module, via a moduleSpec or moduleRef"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    toctoc_att.global.attributes,
    toctoc_att.identified.attributes,
    ## indicates which type of entity should be generated, when an ODD processor is generating a module using XML DTD syntax.
    attribute type {
      ## (parameter entity) 
      "pe" | ## (datatype entity) 
        "dt"
    }?,
    empty
  }

toctoc_remarks =
  ## contains any commentary or discussion about the usage of an element, attribute, class, or entity not otherwise documented within the containing element. [22.4.4. Element Specifications 22.4.5. Attribute List Specification 22.4.6. Element Classes 22.4.7. Pattern Documentation]
