toctoc_specGrp =
  ## (specification group) contains any convenient grouping of specifications for use within the current module.
  element ns1:specGrp {
    (toctoc_model.oddDecl
     | toctoc_model.oddRef
     | toctoc_model.divPart)*,
    toctoc_att.global.attributes,
    empty
  }

toctoc_specGrpRef =
  ## (reference to a specification group) indicates that the declarations contained by the specGrp referenced should be inserted at this point.
  element ns1:specGrpRef {
    empty,
    toctoc_att.global.attributes,
    ## points at the specification group which logically belongs here.
    attribute target { toctoc_data.pointer },
    empty
  }

toctoc_elementSpec =
  ## (element specification) documents the structure, content, and purpose of a single element type. [22.4.4. Element Specifications 22. Documentation Elements]
  element ns1:elementSpec {
    (toctoc_model.glossLike_sequenceOptional,
     toctoc_model.descLike_sequenceOptional,
     toctoc_classes?,
     toctoc_content?,
     toctoc_valList?,
     toctoc_constraintSpec*,
     toctoc_attList?,
     toctoc_exemplum*,
     toctoc_remarks*,
     toctoc_listRef*)
    >> sch:pattern [
         id = "elementSpec-constraint-module-except-when-add1"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:elementSpec"
           "\x{a}" ~
           "               "
           sch:assert [
             test = "@mode"
             "in a customization ODD, the\x{a}" ~
             "		mode= attribute of elementSpec should be\x{a}" ~
             "		specified"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id = "elementSpec-constraint-module-except-when-add1"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:elementSpec"
           "\x{a}" ~
           "               "
           sch:report [
             test = "not( @module ) and not( @mode='add'       )"
             "the module= attribute of elementSpec must be\x{a}" ~
             "		specified anytime the mode= is not 'add'"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id = "elementSpec-constraint-dont-delete-required1"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:elementSpec"
           "\x{a}" ~
           "               "
           sch:report [
