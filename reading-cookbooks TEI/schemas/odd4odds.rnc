toctoc_equiv =
  ## (equivalent) specifies a component which is considered equivalent to the parent element, either by co-reference, or by external link. [22.4.4. Element Specifications 22.4.5. Attribute List Specification 22.4.6. Element Classes 22.4.7. Pattern Documentation]
  element ns1:equiv {
    empty,
    toctoc_att.global.attributes,
    toctoc_att.internetMedia.attributes,
    ## a single word which follows the rules defining a legal XML name (see ), naming the underlying concept of which the parent is a representation.
    attribute name { toctoc_data.name }?,
    ## (uniform resource identifier) references the underlying concept of which the parent is a representation by means of some external identifier
    attribute uri { toctoc_data.pointer }?,
    ## references an external script which contains a method to transform instances of this element to canonical TEI 
    attribute filter { toctoc_data.pointer }?,
    empty
  }

toctoc_altIdent =
  ## (alternate identifier) supplies the recommended XML name for an element, class, attribute, etc. in some language.
  element ns1:altIdent {
    text
    >> sch:pattern [
         id = "altIdent-constraint-altIdent-only-NCName"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:altIdent"
           "\x{a}" ~
           "               "
           sch:assert [
             test =
               "(  parent::tei:attDef       or parent::tei:classSpec       or parent::tei:constraintSpec       or parent::tei:elementSpec       or parent::tei:macroSpec       or parent::tei:moduleSpec       or parent::tei:schemaSpec       ) and       (normalize-space(.) castable as xs:NCName)"
             "\x{a}" ~
             "		  The content of altIdent should be an XML Name (w/o\x{a}" ~
             "		  a namespace prefix), unless a child of valItem\x{a}" ~
             "		(and even then, it's not a bad idea :-)"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    toctoc_att.global.attributes,
    toctoc_att.typed.attributes,
    empty
  }

toctoc_content =
  ## (content model) contains the text of a declaration for the schema documented. [22.4.4. Element Specifications]
  element ns1:content {
    toctoc_macro.schemaPattern*,
    toctoc_att.global.attributes,
    ## controls whether or not pattern names generated in the corresponding RELAXNG schema source are automatically prefixed to avoid potential nameclashes. 
    [ a:defaultValue = "true" ]
    attribute autoPrefix {
      ## Each name referenced in e.g. a rng:ref element within a content model is automatically prefixed by the value of the prefix attribute on the current schemaSpec
      "true" | ## No prefixes are added: any prefix required by the value of the prefix attribute on the current schemaSpec must therefore be supplied explicitly, as appropriate.
        "false"
    }?,
    empty
  }

toctoc_constraint =
  ##  (constraint rules) the formal rules of a constraint
  element ns1:constraint {
    (sch_assert | sch_ns | sch_report | sch_rule | sch_pattern)+,
    toctoc_att.global.attributes,
    empty
  }

toctoc_constraintSpec =
  ##  (constraint on schema) contains a constraint, expressed in some formal syntax, which cannot be expressed in the structural content model [22.4.4. Element Specifications]
  element ns1:constraintSpec {
    toctoc_model.descLike*,
    toctoc_model.glossLike*,
    (toctoc_constraint?)
    >> sch:ns [
         prefix = "s"
         uri = "http://www.ascc.net/xml/schematron"
       ]
    >> sch:pattern [
         id = "constraintSpec-constraint-sch"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:constraintSpec"
           "\x{a}" ~
           "               "
           sch:report [
             test =
               "tei:constraint/s:* and    not(@scheme='schematron')"
             "\x{a}" ~
             "	Rules in the Schematron 1.* language must be inside\x{a}" ~
             "	a constraint with a value of 'schematron' on the scheme attribute\x{a}" ~
             "      "
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:ns [
         prefix = "sch"
         uri = "http://purl.oclc.org/dsdl/schematron"
       ]
    >> sch:pattern [
         id = "constraintSpec-constraint-isosch"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:constraintSpec"
           "\x{a}" ~
           "               "
           sch:report [
             test =
               "tei:constraint/sch:* and    not(@scheme='isoschematron')"
             "\x{a}" ~
             "	Rules in the ISO Schematron language must be inside\x{a}" ~
             "	a constraint with a value of 'isoschematron' on the scheme attribute\x{a}" ~
             "      "
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    toctoc_att.global.attributes,
    toctoc_att.identified.attributes,
    toctoc_att.typed.attributes,
    ## supplies the name of the language in which the constraints are defined
    attribute scheme {
      ##  (Schematron) 
      "schematron" | ##  (ISO Schematron) 
        "isoschematron" | ##  (XSLT) 
        "xsl" | ##  (private constraint language) 
        "private"
    },
    empty
  }

toctoc_attList =
  ## contains documentation for all the attributes associated with this element, as a series of attDef elements. [22.4.4. Element Specifications 22.4.6. Element Classes]
  element ns1:attList {
    (toctoc_attRef | toctoc_attDef | toctoc_attList)+,
    toctoc_att.global.attributes,
    ## (organization) specifies whether all the attributes in the list are available (org="group") or only one of them (org="choice")
    [ a:defaultValue = "group" ]
    attribute org {
      ## grouped
      "group" | ## alternated
        "choice"
    }?,
    empty
  }

toctoc_attDef =
  ## (attribute definition) contains the definition of a single attribute. [22.4.5. Attribute List Specification]
  element ns1:attDef {
    ((toctoc_model.glossLike | toctoc_model.descLike)*,
     toctoc_datatype?,
     toctoc_constraintSpec*,
     toctoc_defaultVal?,
     (toctoc_valList | toctoc_valDesc+)?,
     toctoc_exemplum*,
     toctoc_remarks*)
    >> sch:pattern [
         id = "attDef-constraint-attDefContents"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:attDef"
           "\x{a}" ~
           "               "
           sch:assert [
             test =
               "   @mode eq 'change'       or @mode eq 'delete'       or tei:datatype       or tei:valList[@type='closed']       "
             "Attribute: the definition of the @"
             sch:value-of [ select = "@ident" ]
             " attribute in the "
             sch:value-of [ select = "ancestor::*[@ident][1]/@ident" ]
             "\x{a}" ~
             "                  "
             sch:value-of [ select = "' '" ]
             "\x{a}" ~
             "                  "
             sch:value-of [
               select = "local-name(ancestor::*[@ident][1])"
             ]
             " should have a closed valList or a datatype"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    toctoc_att.global.attributes,
    toctoc_att.identified.attributes,
    ## specifies the optionality of the attribute.
    [ a:defaultValue = "opt" ]
    attribute usage {
      ## (required) 
      "req" | ## (mandatory when applicable ) 
        "mwa" | ## (recommended ) 
        "rec" | ## (recommended when applicable ) 
        "rwa" | ## (optional ) 
        "opt"
    }?,
    ## (namespace) specifies the namespace to which this attribute belongs
    [ a:defaultValue = "http://www.tei-c.org/ns/1.0" ]
    attribute ns { toctoc_data.namespace }?,
    empty
  }

toctoc_attRef =
  ## (attribute pointer) points to the definition of an attribute or group of attributes.
  element ns1:attRef {
    empty,
    toctoc_att.global.attributes,
    ## the name of the pattern defining the attribute(s)
    attribute name { toctoc_data.word },
    empty
  }

toctoc_datatype =
  ## specifies the declared value for an attribute, by referring to any datatype defined by the chosen schema language. [1.4.2. Datatype Macros 22.4.5. Attribute List Specification]
  element ns1:datatype {
    toctoc_macro.schemaPattern*,
    toctoc_att.global.attributes,
    ## (minimum number of occurences) indicates the minimum number of times this datatype may occur in the specification of the attribute being defined
    [ a:defaultValue = "1" ] attribute minOccurs { toctoc_data.count }?,
    ## (maximum number of occurences) indicates the maximum number of times this datatype may occur in the specification of the attribute being defined
    [ a:defaultValue = "1" ]
    attribute maxOccurs { toctoc_data.count | "unbounded" }?,
    empty
  }

toctoc_defaultVal =
  ## (default value) specifies the default declared value for an attribute. [22.4.5. Attribute List Specification]
  element ns1:defaultVal { text, toctoc_att.global.attributes, empty }

toctoc_valDesc =
  ## (value description) specifies any semantic or syntactic constraint on the value that an attribute may take, additional to the information carried by the datatype element. [22.4.5. Attribute List Specification]
  element ns1:valDesc {
    toctoc_macro.phraseSeq.limited,
    toctoc_att.global.attributes,
    toctoc_att.translatable.attributes,
    toctoc_att.combinable.attributes,
    empty
  }

toctoc_valItem =
  ## documents a single value in a predefined list of values. [22.4.5. Attribute List Specification]
  element ns1:valItem {
    (toctoc_model.descLike | toctoc_model.glossLike)*,
    toctoc_att.global.attributes,
    toctoc_att.combinable.attributes,
    ## specifies the value concerned.
    attribute ident { text },
    empty
  }

toctoc_valList =
  ## (value list) contains one or more valItem elements defining possible values. [22.4.5. Attribute List Specification]
  element ns1:valList {
    toctoc_valItem*,
    toctoc_att.global.attributes,
    toctoc_att.combinable.attributes,
    ## specifies the extensibility of the list of values specified. 
    [ a:defaultValue = "open" ]
    attribute type {
      ## only the values specified are permitted.
      "closed" | ## (semi-open) all the values specified should be supported, but other values are legal and software should have appropriate fallback processing for them. 
        "semi" | ## the values specified are sample values only.
        "open"
    },
    empty
  }
toctoc_att.combinable.attributes = toctoc_att.combinable.attribute.mode
toctoc_att.combinable.attribute.mode =
  ## specifies the effect of this declaration on its parent object.
  [ a:defaultValue = "add" ]
  attribute mode {
    ## this declaration is added to the current definitions
    "add" | ## if present already, the whole of the declaration for this object is removed from the current setup
      "delete" | ## this declaration changes the declaration of the same name in the current definition
      "change" | ## this declaration replaces the declaration of the same name in the current definition
      "replace"
  }?
toctoc_att.identified.attributes =
  toctoc_att.combinable.attributes,
  toctoc_att.identified.attribute.module,
  toctoc_att.identified.attribute.ident,
  toctoc_att.identified.attribute.predeclare,
  toctoc_att.identified.attribute.status

toctoc_att.identified.attribute.module =
  attribute module {
    "analysis" | "certainty" | "core" | "corpus" | "dictionaries" | "drama" | "figures" | "gaiji" | "header" | "iso-fs" | "linking" | "msdescription" | "namesdates" | "nets" | "spoken" | "tagdocs" | "tei" | "textcrit" | "textstructure" | "transcr" | "verse"
  }?
toctoc_att.identified.attribute.ident =
  ## supplies the identifier by which this element may be referenced.
  attribute ident { toctoc_data.name }
toctoc_att.identified.attribute.predeclare =
  ## says whether this object should be predeclared in the tei infrastructure module.
  [ a:defaultValue = "false" ]
  attribute predeclare { toctoc_data.truthValue }?
toctoc_att.identified.attribute.status =
  ## indicates the current status of the object identified with respect to the current version of the TEI Guidelines.
  [ a:defaultValue = "stable" ]
  attribute status {
    ## the item is not recommended for use, and may be withdrawn at a future release.
    "deprecated" | ## the item is new and still under review.
      "unstable" | ## the item has changed significantly since the preceding version.
      "changed" | ## the item has not recently changed and is not expected to do so except for correction of any errors.
      "stable"
  }?
toctoc_att.namespaceable.attributes =
  toctoc_att.namespaceable.attribute.ns
toctoc_att.namespaceable.attribute.ns =
  ## (namespace) specifies the namespace to which this element belongs
  [ a:defaultValue = "http://www.tei-c.org/ns/1.0" ]
  attribute ns { toctoc_data.namespace }?
toctoc_att.datable.custom.attributes =
  toctoc_att.datable.custom.attribute.when-custom,
  toctoc_att.datable.custom.attribute.notBefore-custom,
  toctoc_att.datable.custom.attribute.notAfter-custom,
  toctoc_att.datable.custom.attribute.from-custom,
  toctoc_att.datable.custom.attribute.to-custom,
  toctoc_att.datable.custom.attribute.datingPoint,
  toctoc_att.datable.custom.attribute.datingMethod
toctoc_att.datable.custom.attribute.when-custom =
  ## supplies the value of a date or time in some standard form.
  attribute when-custom {
    list { toctoc_data.word+ }
  }?
toctoc_att.datable.custom.attribute.notBefore-custom =
  ## specifies the earliest possible date for the event in some custom standard form.
  attribute notBefore-custom {
    list { toctoc_data.word+ }
  }?
toctoc_att.datable.custom.attribute.notAfter-custom =
  ## specifies the latest possible date for the event in some custom standard form.
  attribute notAfter-custom {
    list { toctoc_data.word+ }
  }?
toctoc_att.datable.custom.attribute.from-custom =
  ## indicates the starting point of the period in some standard form.
  attribute from-custom {
    list { toctoc_data.word+ }
  }?
toctoc_att.datable.custom.attribute.to-custom =
  ## indicates the ending point of the period in some standard form.
  attribute to-custom {
    list { toctoc_data.word+ }
  }?
toctoc_att.datable.custom.attribute.datingPoint =
  ## supplies a pointer to some location defining a named point in time with reference to which the datable item is understood to have occurred
  attribute datingPoint { toctoc_data.pointer }?
toctoc_att.datable.custom.attribute.datingMethod =
  ## supplies a pointer to a calendar element or other means of interpreting the values of the custom dating attributes.
  attribute datingMethod { toctoc_data.pointer }?
toctoc_att.datable.iso.attributes =
  toctoc_att.datable.iso.attribute.when-iso,
  toctoc_att.datable.iso.attribute.notBefore-iso,
  toctoc_att.datable.iso.attribute.notAfter-iso,
  toctoc_att.datable.iso.attribute.from-iso,
  toctoc_att.datable.iso.attribute.to-iso
toctoc_att.datable.iso.attribute.when-iso =
  ## supplies the value of a date or time in a standard form.
  attribute when-iso { toctoc_data.temporal.iso }?
toctoc_att.datable.iso.attribute.notBefore-iso =
  ## specifies the earliest possible date for the event in standard form, e.g. yyyy-mm-dd.
  attribute notBefore-iso { toctoc_data.temporal.iso }?
toctoc_att.datable.iso.attribute.notAfter-iso =
  ## specifies the latest possible date for the event in standard form, e.g. yyyy-mm-dd.
  attribute notAfter-iso { toctoc_data.temporal.iso }?
toctoc_att.datable.iso.attribute.from-iso =
  ## indicates the starting point of the period in standard form.
  attribute from-iso { toctoc_data.temporal.iso }?
toctoc_att.datable.iso.attribute.to-iso =
  ## indicates the ending point of the period in standard form.
  attribute to-iso { toctoc_data.temporal.iso }?

toctoc_orgName =
  ## (organization name) contains an organizational name.  [13.2.2. Organizational Names]
  element ns1:orgName {
    toctoc_macro.phraseSeq,
    toctoc_att.global.attributes,
    toctoc_att.datable.attributes,
    toctoc_att.editLike.attributes,
    toctoc_att.personal.attributes,
    toctoc_att.typed.attributes,
    empty
  }

toctoc_persName =
  ## (personal name) contains a proper noun or proper-noun phrase referring to a person, possibly including one or more of the person's forenames, surnames, honorifics, added names, etc. [13.2.1. Personal Names]
  element ns1:persName {
    toctoc_macro.phraseSeq,
    toctoc_att.global.attributes,
    toctoc_att.datable.attributes,
    toctoc_att.editLike.attributes,
    toctoc_att.personal.attributes,
    toctoc_att.typed.attributes,
    empty
  }

toctoc_placeName =
  ## contains an absolute or relative place name.  [13.2.3. Place Names]
  element ns1:placeName {
    toctoc_macro.phraseSeq,
    toctoc_att.datable.attributes,
    toctoc_att.editLike.attributes,
    toctoc_att.global.attributes,
    toctoc_att.personal.attributes,
    toctoc_att.typed.attributes,
    empty
  }

toctoc_table =
  ## contains text displayed in tabular form, in rows and columns. [14.1.1. TEI Tables]
  element ns1:table {
    ((toctoc_model.headLike | toctoc_model.global)*,
     ((toctoc_row, toctoc_model.global*)+
      | (toctoc_model.graphicLike, toctoc_model.global*)+),
     (toctoc_model.divBottom, toctoc_model.global*)*),
    toctoc_att.global.attributes,
    ## indicates the number of rows in the table.
    attribute rows { toctoc_data.count }?,
    ## (columns) indicates the number of columns in each row of the table.
    attribute cols { toctoc_data.count }?,
    empty
  }

toctoc_row =
  ## contains one row of a table.  [14.1.1. TEI Tables]
  element ns1:row {
    toctoc_cell+,
    toctoc_att.global.attributes,
    toctoc_att.tableDecoration.attributes,
    empty
  }

toctoc_cell =
  ## contains one cell of a table.  [14.1.1. TEI Tables]
  element ns1:cell {
    toctoc_macro.paraContent,
    toctoc_att.global.attributes,
    toctoc_att.tableDecoration.attributes,
    empty
  }

toctoc_formula =
  ## contains a mathematical or other formula. [14.2. Formul and
  ## Mathematical Expressions]
  element ns1:formula {
    (text | toctoc_model.graphicLike | toctoc_model.hiLike)*,
    toctoc_att.global.attributes,
    ## names the notation used for the content of the element.
    attribute notation { toctoc_data.enumerated }?,
    empty
  }

toctoc_figure =
  ## groups elements representing or containing graphic information such as an illustration, formula, or figure. [14.4. Specific Elements for Graphic Images]
  element ns1:figure {
    (toctoc_model.headLike
     | toctoc_model.common
     | toctoc_figDesc
     | toctoc_model.graphicLike
     | toctoc_model.global
     | toctoc_model.divBottomPart)*,
    toctoc_att.global.attributes,
    toctoc_att.placement.attributes,
    toctoc_att.typed.attributes,
    empty
  }

toctoc_figDesc =
  ## (description of figure) contains a brief prose description of the appearance or content of a graphic figure, for use when documenting an image without displaying it. [14.4. Specific Elements for Graphic Images]
  element ns1:figDesc {
    toctoc_macro.limitedContent, toctoc_att.global.attributes, empty
  }

toctoc_fallback =
  element ns2:fallback { (text | toctoc_macro.anyXML)+ }
start =
  toctoc_TEI
  | toctoc_body
  | toctoc_schemaSpec
  | toctoc_elementSpec
  | toctoc_classSpec
  | toctoc_macroSpec
  | toctoc_div
