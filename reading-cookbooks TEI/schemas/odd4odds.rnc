toctoc_moduleSpec =
  ## (module specification) documents the structure, content, and purpose of a single module, i.e. a named and externally visible group of declarations.
  element ns1:moduleSpec {
    (toctoc_model.glossLike*,
     toctoc_model.descLike+,
     toctoc_exemplum*,
     toctoc_remarks?,
     toctoc_listRef*),
    toctoc_att.global.attributes,
    toctoc_att.identified.attributes,
    toctoc_att.typed.attributes,
    empty
  }

toctoc_schemaSpec =
  ## (schema specification) generates a TEI-conformant schema and documentation for it.
  element ns1:schemaSpec {
    (toctoc_model.glossLike_sequenceOptional,
     toctoc_model.descLike_sequenceOptional,
     (toctoc_model.oddRef | toctoc_model.oddDecl)*,
     toctoc_constraintSpec*)
    >> sch:pattern [
         id = "schemaSpec-constraint-required-modules"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:schemaSpec"
           "\x{a}" ~
           "               "
           sch:assert [
             test =
               "tei:moduleRef[@key='tei']       and       tei:moduleRef[@key='core']      and       tei:moduleRef[@key='header']    and       tei:moduleRef[@key='textstructure']"
             "\x{a}" ~
             "		  missing one or more of the required modules (tei, core, header, textstructure).\x{a}" ~
             "		"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id = "schemaSpec-constraint-no-outside-specs"
         "\x{a}" ~
         "            "
         sch:rule [
           context =
             "tei:classSpec[not(ancestor::tei:schemaSpec)]        | tei:elementSpec[not(ancestor::tei:schemaSpec)]        | tei:macroSpec[not(ancestor::tei:schemaSpec)]        "
           "\x{a}" ~
           "                  "
           sch:assert [
             test =
               "//tei:schemaSpec//tei:specGrpRef[         substring-after( normalize-space(@target),'#') =         current()/@xml:id]"
             ""
             sch:name [ ]
             " should be in schemaSpec\x{a}" ~
             "		  or referred to from within schemaSpec"
           ]
           "\x{a}" ~
           "                "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id = "schemaSpec-constraint-only-one-schemaSpec"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "/"
           "\x{a}" ~
           "                  "
           sch:report [
             test = "count( //tei:schemaSpec ) eq 0"
             "There's no schemaSpec, so this isn't much of a TEI customization"
           ]
           "\x{a}" ~
           "                  "
           sch:report [
             test = "count( //tei:schemaSpec ) gt 1"
             "You have more than one schemaSpec; current ODD processors will only look at the first one"
           ]
           "\x{a}" ~
           "                "
         ]
         "\x{a}" ~
         "         "
       ],
