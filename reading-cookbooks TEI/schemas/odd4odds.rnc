toctoc_att.styleDef.attributes = toctoc_att.styleDef.attribute.scheme
toctoc_att.styleDef.attribute.scheme =
  ## identifies the language used to describe the rendition.
  attribute scheme {
    ## Cascading Stylesheet Language
    "css" | ## Extensible Stylesheet Language Formatting Objects
      "xslfo" | ## Informal free text description
      "free" | ## A user-defined rendition description language
      "other"
  }?
toctoc_att.tableDecoration.attributes =
  toctoc_att.tableDecoration.attribute.role,
  toctoc_att.tableDecoration.attribute.rows,
  toctoc_att.tableDecoration.attribute.cols
toctoc_att.tableDecoration.attribute.role =
  ## indicates the kind of information held in this cell or in each cell of this row.
  ## Suggested values include: 1] label; 2] data
  [ a:defaultValue = "data" ]
  attribute role {
    ## labelling or descriptive information only.
    "label" | ## data values.
      "data"
    | xsd:Name
  }?
toctoc_att.tableDecoration.attribute.rows =
  ## indicates the number of rows occupied by this cell or row.
  [ a:defaultValue = "1" ] attribute rows { toctoc_data.count }?
toctoc_att.tableDecoration.attribute.cols =
  ## (columns) indicates the number of columns occupied by this cell or row.
  [ a:defaultValue = "1" ] attribute cols { toctoc_data.count }?
toctoc_att.timed.attributes =
